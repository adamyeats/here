{
  "name": "here",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.1"
  },
  "workspaces": [
    "./client",
    "./server"
  ],
  "scripts": {
    "start": "node server/app.js",
    "build": "npm run build -w client",
    "build:watch": "concurrently \"npm run dev -w server\" \"PORT=3001 npm run start -w client\""
  },
  "engines": {
    "node": "16.x"
  },
  "volta": {
    "node": "16.3.0",
    "npm": "7.17.0"
  }
}
